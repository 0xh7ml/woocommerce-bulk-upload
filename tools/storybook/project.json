{
	"root": "woocommerce-storybook",
	"sourceRoot": "tools/storybook",
	"projectType": "library",
	"targets": {
		"changelog": {
			"executor": "./tools/executors/changelogger:changelog",
			"options": {
				"action": "add",
				"cwd": "packages/js/admin-e2e-tests"
			}
		},
		"storybook": {
			"executor": "@nrwl/workspace:run-commands",
			"options": {
				"command": "pnpm run storybook",
				"cwd": "tools/storybook"
			}
		}
	}
}
